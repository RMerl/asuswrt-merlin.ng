# Linux Kernel wl driver config file

# get basic configuration
include $(WLCFGDIR)/wlconfig_lx_router_apsta

# features
WL_EAP_AP=1
WL_EAP_STA_SCB_TIMEOUT=1
WL_EAP_PER_VAP_CONFIG=1
WL_EAP_PER_VAP_PKTC=1
WL_EAP_SCAN_TX=1
WL_EAP_UCODE=1
WL_EAP_UCODE_TX_DESC=0
WL_EAP_EMSGLVL=1
#WL_EAP_PKTTAG_EXT=1
WL_EAP_KEY_CACHE=1
WL_EAP_SCAN_MEASUREMENT=1
WL_EAP_SCAN_TEST=1
WL_EAP_SCAN_PROTECT=1
WL_EAP_SCAN_BEACON_DELAY=1
WL_EAP_DATA_SNOOP=1
WL_EAP_NOISE_MEASUREMENTS=1
WL_EAP_RXTX_UTILIZATION=1
WL_EAP_MONITOR=1
#WL_EAP_80211RAW=1
#WL_EAP_80211RAW_TEST=1
WL_EAP_DROP_RX_MGMT_RSSI=1
WL_EAP_SNR=1
#WL_EAP_OBJMEM_DBG=1
WL_MCAST_FILTER_NOSTA=1

#WL_EAP_SCAN_TX dependent feature
ifeq ($(WL_EAP_SCAN_TX),1)
#WL_EAP_SCAN_TX_MAX_POWER=1
endif

#WLOLPC dependent feature
ifeq ($(WLOLPC),1)
WL_EAP_OLPC=1
endif

#SAMPLE_COLLECT dependent feature
ifeq ($(SAMPLE_COLLECT),1)
WL_EAP_SAMPLE_COLLECT=1
endif

#WL_EAP_UCODE dependent features
ifeq ($(WL_EAP_UCODE),1)
WL_EAP_PER_VAP_DTIM=1
WL_EAP_RUNT_ON_BADFCS_FRM=1
WL_EAP_PER_VAP_AMSDU_HWDAGG_DIS=1
WL_EAP_OUTDOOR_AP=1
WL_EAP_ALLOW_MESH_FRM=1
WL_EAP_ALLOW_MGMT_FRM=1
WL_EAP_ALLOW_SOUND_FB=1
WL_EAP_PER_VAP_CONFIG_RATESET=1
WL_EAP_FFT_SAMPLE=1
WL_EAP_ACK_RSSI=1
WL_EAP_FIPS_LOOPBACK=1
endif

# WL_EAP_CUSTOM_SCAN requires WL_EAP_EVENT_SERVICE and WL_EAP_AP
WL_EAP_EVENT_SERVICE=1
WL_EAP_CUSTOM_SCAN=1

WL_EAP_MBSS_BCNROTATE=1
WL_EAP_PRS_RTX_LIMIT=1
WL_EAP_STATS=1

# Support for board-specific hardware RF analog band-pass filters for
# 5G channel isolation
WL_EAP_BOARD_RF_5G_FILTER=1

# Save RSSI of most recently rcvd frame
WL_EAP_LAST_PKT_RSSI=1

# EAP debug features
WL_EAP_TPDUMP=1

# other debug
CHAN_SWITCH_HIST=1

# WL_EAP_CUST_EVENT_HNDLR requires WL_EAP_EVENT_SERVICE
ifeq ($(WL_EAP_EVENT_SERVICE),1)
WL_EAP_CUST_EVENT_HNDLR=1
endif

#AIRIQ feature
ifeq ($(CONFIG_AIRIQ),y)
WL_AIRIQ=1
endif

# Location/proxd/FTM feature
WL_PROXDETECT=1
WL_FTM=1

#
# Capability to control whether AP should change channels automatically
# after a radar detection event.  If not set then AP will ALWAYS change
# channels automatically and not give this as a configurable option.
# Defining this makes it configurable one way or the other
#
WL_EAP_DFS_ALTMODE=1

#Use Uplink PKTC only within wlan driver; unchain/clear before sending up
WL_EAP_WLAN_ONLY_UL_PKTC=0

#Offload SKB free from WLAN thread to skb_free_task on another core
BCM_SKB_FREE_OFFLOAD=1

# Packets counters at different stages in wl driver
WL_EAP_PKTCNTR=1

#Poll idle STAs with frame like BAR
WL_EAP_POLL_IDLE_STA=1

# 43570 support
WL_EAP_BCM43570=1

# Disable CFP
WLCFP=0
# Use supr_psq for PS suppressed frames
WL_EAP_USE_SUPR_PSQ=0

# Disable VHT proprietary rates
WL_EAP_VHT_PROPRIETARY_RATES_DIS=1
