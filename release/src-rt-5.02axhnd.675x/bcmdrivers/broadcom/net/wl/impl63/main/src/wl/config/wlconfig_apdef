# AP features

# Allow these to be overridden via the environment.
# Router makefile make use of these features.
MFP      = $(if $(findstring y,$(CONFIG_MFP)),1,0)
HSPOT    = $(if $(findstring y,$(CONFIG_HSPOT)),1,0)
WLWNM_AP = $(if $(findstring y,$(CONFIG_WNM)$(CONFIG_HSPOT)),1,0)
WLFBT    = $(if $(findstring y,$(CONFIG_FBT)),1,0)
BCM_CEVENT = $(if $(findstring y,$(CONFIG_BCM_CEVENT)),1,0)
MBO_AP    = $(if $(findstring y,$(CONFIG_MBO_AP)),1,0)
WL_MBO    = $(if $(findstring 1,$(MBO_AP)),1,0)
WL_AP_CFG80211 = $(if $(findstring y,$(CONFIG_BCM_HOSTAPD)),1,0)
WL_DPP = $(if $(findstring y,$(CONFIG_BCM_HOSTAPD)),1,0)
AP=1
WDS=1
DWDS=1
BCMCCMP=1
WLCNTSCB=1
MBSS=1
WLPROBRESP_SW=1
WME_PER_AC_TX_PARAMS=1
WME_PER_AC_TUNING=1
WLOVERTHRUSTER=1
BCMECICOEX=0
