<object name="InternetGatewayDevice.X_BROADCOM_COM_StandbyCfg." shortObjectName="StandbyCfgObject" specSource="Broadcom" profile="X_BROADCOM_COM_StandBy:1" supportLevel="Present" />
  <description source="BROADCOM">A Power Saving feature that allows configuring a time when the system or specific interfaces are put to sleep and a time to wake up.</description>

  <parameter name="StandbyEnable" type="boolean" specSource="Broadcom" profile="X_BROADCOM_COM_StandBy:1" supportLevel="ReadWrite" defaultValue="false" />
    <description source="BROADCOM">To enable or disable the Standby feature. When enabled and Standby Time and Wake Up Time are specified, the system goes in Standby at Standby Time and wakes up at Wake Up time.</description>

  <parameter name="StandbyStatusString" type="string" specSource="Broadcom" profile="X_BROADCOM_COM_StandBy:1" supportLevel="ReadWrite" defaultValue="Disabled" validValuesArray="StandbyStateValues" />

  <parameter name="StandbyHour" type="int" specSource="Broadcom" profile="X_BROADCOM_COM_StandBy:1" supportLevel="ReadWrite" defaultValue="0" minValue="0" maxValue="23" />
    <description source="BROADCOM">The hour at which Standby starts.</description>

  <parameter name="StandbyMinutes" type="int" specSource="Broadcom" profile="X_BROADCOM_COM_StandBy:1" supportLevel="ReadWrite" defaultValue="0" minValue="0" maxValue="59" />
    <description source="BROADCOM">The minutes when Standby Starts</description>

  <parameter name="WakeupHour" type="int" specSource="Broadcom" profile="X_BROADCOM_COM_StandBy:1" supportLevel="ReadWrite" defaultValue="0" minValue="0" maxValue="23" />
    <description source="BROADCOM">The hour when Standby finishes.</description>

  <parameter name="WakeupMinutes" type="int" specSource="Broadcom" profile="X_BROADCOM_COM_StandBy:1" supportLevel="ReadWrite" defaultValue="0" minValue="0" maxValue="59" />
    <description source="BROADCOM">The minutes when Standby ends.</description>


