/dts-v1/;
#include "at91sam9g25.dtsi"
#include "at91sam9x5ek.dtsi"

/ {
	model = "Atmel AT91SAM9G25-EK";
	compatible = "atmel,at91sam9g25ek", "atmel,at91sam9x5ek", "atmel,at91sam9x5", "atmel,at91sam9";

	ahb {
		apb {
			spi0: spi@f0000000 {
				status = "disabled";
			};

			mmc1: mmc@f000c000 {
				status = "disabled";
			};

			i2c0: i2c@f8010000 {
				ov2640: camera@0x30 {
					status = "okay";
				};
			};

			macb0: ethernet@f802c000 {
				phy-mode = "rmii";
				status = "okay";
			};

			isi: isi@f8048000 {
				status = "okay";
			};
		};
	};
};
