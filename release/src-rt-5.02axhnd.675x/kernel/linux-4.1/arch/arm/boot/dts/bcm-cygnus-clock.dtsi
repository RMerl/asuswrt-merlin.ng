clocks {
	#address-cells = <1>;
	#size-cells = <1>;
	ranges;

	osc: oscillator {
		compatible = "fixed-clock";
		#clock-cells = <1>;
		clock-frequency = <25000000>;
	};

	apb_clk: apb_clk {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <1000000000>;
	};

	periph_clk: periph_clk {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <500000000>;
	};

	sdio_clk: lcpll_ch2 {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <200000000>;
	};

	axi81_clk: axi81_clk {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <100000000>;
	};

	keypad_clk: keypad_clk {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <31806>;
	};

	adc_clk: adc_clk {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <1562500>;
	};

	pwm_clk: pwm_clk {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <1000000>;
	};

	lcd_clk: mipipll_ch1 {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <100000000>;
	};
};
