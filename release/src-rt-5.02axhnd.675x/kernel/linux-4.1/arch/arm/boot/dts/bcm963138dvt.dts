/*
 * Broadcom BCM63138 Reference Board DTS
 */

/dts-v1/;

#include "bcm63138.dtsi"

/ {
	compatible = "brcm,BCM963138DVT", "brcm,bcm63138";
	model = "Broadcom BCM963138DVT";

	chosen {
		bootargs = "console=ttyS0,115200";
		stdout-path = &serial0;
	};

	memory {
		reg = <0x0 0x08000000>;
	};

};

&serial0 {
	status = "okay";
};

&serial1 {
	status = "okay";
};
