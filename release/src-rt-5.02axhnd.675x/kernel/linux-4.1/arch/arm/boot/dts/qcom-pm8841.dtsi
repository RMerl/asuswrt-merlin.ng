#include <dt-bindings/spmi/spmi.h>

&spmi_bus {

	usid4: pm8841@4 {
		compatible = "qcom,spmi-pmic";
		reg = <0x4 SPMI_USID>;
		#address-cells = <1>;
		#size-cells = <0>;
	};

	usid5: pm8841@5 {
		compatible = "qcom,spmi-pmic";
		reg = <0x5 SPMI_USID>;
		#address-cells = <1>;
		#size-cells = <0>;
	};
};
