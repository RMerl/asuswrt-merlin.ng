/dts-v1/;

/include/ "apm-storm.dtsi"

/ {
	model = "APM X-Gene Mustang board";
	compatible = "apm,mustang", "apm,xgene-storm";

	chosen { };

	memory {
		device_type = "memory";
		reg = < 0x1 0x00000000 0x0 0x80000000 >; /* Updated by bootloader */
	};
};

&pcie0clk {
	status = "ok";
};

&pcie0 {
	status = "ok";
};

&serial0 {
	status = "ok";
};

&menet {
	status = "ok";
};

&sgenet0 {
	status = "ok";
};

&sgenet1 {
	status = "ok";
};

&xgenet {
	status = "ok";
};
