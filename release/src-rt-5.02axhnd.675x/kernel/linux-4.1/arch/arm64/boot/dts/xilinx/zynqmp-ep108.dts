/dts-v1/;

/include/ "zynqmp.dtsi"

/ {
	model = "ZynqMP EP108";

	aliases {
		serial0 = &uart0;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x0 0x40000000>;
	};
};

&gem0 {
	status = "okay";
	phy-handle = <&phy0>;
	phy-mode = "rgmii-id";
	phy0: phy@0{
		reg = <0>;
		max-speed = <100>;
	};
};

&uart0 {
	status = "okay";
};
