obj-y += prom.o time.o clock.o platform.o power.o \
	 setup.o sleeper.o dma.o dbdma.o vss.o irq.o usb.o

# optional gpiolib support
ifeq ($(CONFIG_ALCHEMY_GPIO_INDIRECT),)
 obj-$(CONFIG_GPIOLIB) += gpiolib.o
endif
