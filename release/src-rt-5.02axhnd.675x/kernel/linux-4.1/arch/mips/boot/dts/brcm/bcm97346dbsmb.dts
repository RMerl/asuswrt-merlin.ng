/dts-v1/;

/include/ "bcm7346.dtsi"

/ {
	compatible = "brcm,bcm97346dbsmb", "brcm,bcm7346";
	model = "Broadcom BCM97346DBSMB";

	memory@0 {
		device_type = "memory";
		reg = <0x00000000 0x10000000>, <0x20000000 0x30000000>;
	};

	chosen {
		bootargs = "console=ttyS0,115200";
		stdout-path = &uart0;
	};
};

&uart0 {
	status = "okay";
};

&enet0 {
	status = "okay";
};

&ehci0 {
	status = "okay";
};

&ohci0 {
	status = "okay";
};

&ehci1 {
	status = "okay";
};

&ohci1 {
	status = "okay";
};

&ehci2 {
	status = "okay";
};

&ohci2 {
	status = "okay";
};

&ehci3 {
	status = "okay";
};

&ohci3 {
	status = "okay";
};
