mainmenu_option next_comment
comment 'Toolchain selection'

if [ "$BRCM_6858" = "y" -o "$BRCM_4908" = "y" -o "$BRCM_6836" = "y" -o "$BRCM_6856" = "y" -o "$BRCM_63158" = "y" ]; then
    choice 'Userspace build architecture ' \
        "arm         USER_ARCH_ARM
         aarch64     USER_ARCH_AARCH64" arm

    if [ "$USER_ARCH_AARCH64" = "y" ]; then
        define_string PROFILE_ARCH "aarch64"
    else
        define_string PROFILE_ARCH "arm"
    fi
    define_string KERNEL_ARCH "aarch64"
else
    if [ "$BRCM_63138" = "y" -o "$BRCM_63148" = "y" -o "$BRCM_47189" = "y" -o "$BRCM_6846" = "y" ]; then
        define_string PROFILE_ARCH "arm"
        define_string KERNEL_ARCH "arm"
    else
        define_string PROFILE_ARCH "mips"
    fi
fi

if [ "$PROFILE_ARCH" = "arm" ]; then
    choice ' Base Library' \
        "   UCLIBC              BRCM_UCLIBC
            GLIBC               BRCM_GLIBC" UCLIBC
else
    if [ "$PROFILE_ARCH" = "aarch64" ]; then
        choice ' Base Library' \
            "   GLIBC              BRCM_GLIBC" GLIBC
    else
        choice ' Base Library' \
            "   UCLIBC              BRCM_UCLIBC" UCLIBC
    fi        
fi

endmenu



###################################################################################

mainmenu_option next_comment
comment 'File system selection'

choice 'Root file system '   \
      "Legacy_SQUASHFS_Presets                BRCM_ROOTFS_SQUASHFS   \
       Legacy_JFFS2+UBIFS+SQUASHFS_Presets     BRCM_ROOTFS_ALL \
       Configurable BRCM_ROOTFS_CONFIG" BRCM_ROOTFS_CONFIG

if [ "$BRCM_ROOTFS_CONFIG" = "y" ]; then
    bool '    Build NAND Images' BRCM_FLASH_BUILD_NAND 
    if [ "$BRCM_FLASH_BUILD_NAND" = "y" ]; then
        if [ "$BRCM_6846" != "y" ]; then
            bool '        Split JFFS2/UBI layout (deprecated)' BRCM_FLASH_NAND_LAYOUT_SPLIT
        fi
        bool '        Pure UBI layout' BRCM_FLASH_NAND_LAYOUT_PUREUBI
        bool '        NAND ROOTFS Filesystem UBIFS' BRCM_FLASH_NAND_ROOTFS_UBIFS
        bool '        NAND ROOTFS Filesystem SQUASHFS' BRCM_FLASH_NAND_ROOTFS_SQUBI
    fi
    bool '    Build eMMC Images' BUILD_EMMC_IMG 
    if [ "$BUILD_EMMC_IMG" = "y" ]; then
        bool '        eMMC ROOTFS Filesystem EXT4' BRCM_FLASH_EMMC_ROOTFS_EXT4
        bool '        eMMC ROOTFS Filesystem SQUASHFS' BRCM_FLASH_EMMC_ROOTFS_SQUASHFS
        define_bool BUILD_E2FSPROGS "y"
        define_bool BUILD_GPTFDISK "y"
    fi
fi

if [ "$BRCM_ROOTFS_ALL" = "y" ]; then
    define_string BRCM_FLASH_BUILD_NOR "y"
    define_string BRCM_FLASH_BUILD_NAND "y"
    if [ "$BRCM_6846" != "y" -a "$BRCM_6836" != "y" -a "$BRCM_6856" != "y" ]; then
        define_string BRCM_FLASH_NAND_LAYOUT_SPLIT "y"
    fi
    define_string BRCM_FLASH_NAND_LAYOUT_PUREUBI "y"
    define_string BRCM_FLASH_NAND_ROOTFS_UBIFS "y"
    define_string BRCM_FLASH_NAND_ROOTFS_SQUBI "y"
    if [ "$BRCM_DRIVER_EMMC" = "y" ]; then
        define_string BUILD_EMMC_IMG "y"
        define_string BRCM_FLASH_EMMC_ROOTFS_EXT4 "y"
        define_string BRCM_FLASH_EMMC_ROOTFS_SQUASHFS "y"
        define_bool BUILD_E2FSPROGS "y"
        define_bool BUILD_GPTFDISK "y"
    fi
fi

if [ "$BUILD_GPTFDISK" = "y" ]; then
    define_bool BUILD_LIBPOPT "y"
    define_bool BUILD_LIBUUID "y"
fi

if [ "$BRCM_ROOTFS_SQUASHFS" = "y" ]; then
    define_string BRCM_FLASH_BUILD_NOR "y"
fi

if [ "$BRCM_FLASH_BUILD_NOR" = "y" ]; then
int '    SquashFS fragment cache size in # of blocks' BRCM_SQUASHFS_FRAGMENT_CACHE_SIZE 1
choice 'NOR Flash Kernel Image Compression '   \
        "LZMA                BUILD_NOR_KERNEL_LZMA   \
        LZ4                  BUILD_NOR_KERNEL_LZ4" LZMA

comment 'Auxillary flash file system selection (on NOR)'

bool '   JFFS2' BUILD_AUXFS_JFFS2
if  [ "$BUILD_AUXFS_JFFS2" = "y" ]; then
    define_string BRCM_KERNEL_AUXFS_JFFS2 "y"
fi
int 'Percent of NOR Flash for auxillary file system 0-80' BRCM_AUXFS_PERCENT

fi

if [ "$BRCM_FLASH_BUILD_NAND" = "y" ]; then
    define_string BRCM_KERNEL_ROOTFS "all"
    mainmenu_option next_comment
    comment 'NAND Options '
    bool '    NAND image with lzma kernel'  BUILD_NAND_KERNEL_LZMA
    bool '    NAND image with lz4 kernel'   BUILD_NAND_KERNEL_LZ4
    if [ "$BRCM_63268" = "y" -o "$BRCM_63381" = "y" ]; then
        bool '    NAND image with 16KB block size' BUILD_NAND_IMG_BLKSIZE_16KB
    fi
    bool '    NAND image with 128KB block size' BUILD_NAND_IMG_BLKSIZE_128KB
    bool '    NAND image with 256KB block size' BUILD_NAND_IMG_BLKSIZE_256KB
    bool '    NAND image with 512KB block size' BUILD_NAND_IMG_BLKSIZE_512KB
    bool '    NAND image with 1024KB block size' BUILD_NAND_IMG_BLKSIZE_1024KB
    comment ''
    bool '    Mount UBI ROOTFS as read-write (required by Modular Software Update and OSGi)' BRCM_ROOTFS_RW
    bool '    Include MTD UTILS diagnostics on device' BUILD_MTDUTILS 
    endmenu
else
    define_string BRCM_KERNEL_ROOTFS "squashfs"
fi
int 'MISC1 partition SIZE' BRCM_MISC1_PARTITION_SIZE 0
int 'MISC2 partition SIZE' BRCM_MISC2_PARTITION_SIZE 0
int 'MISC3 partition SIZE' BRCM_MISC3_PARTITION_SIZE 0
int 'MISC4(Data) partition SIZE' BRCM_MISC4_PARTITION_SIZE 4

bool 'Configure User Partitions' BRCM_CFG_USER_PARTITIONS
if [ "$BRCM_CFG_USER_PARTITIONS" = "y" ]; then
   string 'Name of Partition Configuration File' BRCM_PARTITION_CFG_FILE
fi


bool 'Enable boot from RAMDISK support' BRCM_RAMDISK_BOOT_EN
if [ "$BRCM_RAMDISK_BOOT_EN" = "y" ]; then
    int '    RAMDISK size (if not used, kernel will release it automatically)' BRCM_RAMDISK_SIZE 8192
fi

bool 'Enable NFS Mount' BRCM_NFS_MOUNT_EN

bool 'Enable Incremental Image loading' BRCM_INCREMENTAL_IMAGE_LOAD

bool 'Delay commit of image until successful reboot to Linux [instead of default commit image after write]' BRCM_NO_AUTOCOMMIT_IMAGE

endmenu


