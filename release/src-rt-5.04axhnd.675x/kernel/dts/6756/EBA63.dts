    switch_sf2_ext:0 {
    };
};

&mdio_sf2 {
	phy_ge:8 {
		status = "okay";
	};
};

&switch0 {
	ports {
		port0@0 {
            phy-handle = <&phy_ge>;
			status = "okay";
		};
    };
};

&led_ctrl {
	status="okay";

	/* LED_R */
	led0:sw_led_14 {
		active_low;
		pinctrl-0=<&a_per_led_14_pin_14>;
		status = "okay";
		brightness = <0>;
	};
	/* LED_G */
	led1:sw_led_15 {
		active_high;
		pinctrl-0=<&a_per_led_15_pin_15>;
		status = "okay";
		brightness = <255>;
	};
	/* LED_B */
	led2:sw_led_16 {
		active_low;
		pinctrl-0=<&a_per_led_16_pin_16>;
		status = "okay";
		brightness = <0>;
	};

	/* LED_W */
	led3:led_gpio_17 {
		compatible = "brcm,gpio-led";
		//label = "LED_W";
		software_led;
		pin = <17>;
		active_low;
		init_high;
	};

	/* Fake 5V_4V5_REG_Set as LED */
	V_REG_Set:led_gpio_22 {
		compatible = "brcm,gpio-led";
		label = "V_REG_Set";
		software_led;
		pin = <22>;
		active_low;
		init_high;
	};
};

&usb_ctrl {
    pinctrl-names="default";
    pinctrl-0 = <&usb0_pwr_pins>;
    status = "okay";
    xhci-enable;
};

&usb0_xhci {
    status = "okay";
};

&usb0_ehci {
    status = "okay";
};

&usb0_ohci {
    status = "okay";
};

&pcie0 {
    status = "disabled";
};
