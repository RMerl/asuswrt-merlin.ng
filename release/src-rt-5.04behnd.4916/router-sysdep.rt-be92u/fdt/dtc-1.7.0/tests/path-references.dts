/dts-v1/;

/ {
	rref = &{/};
	/* Check multiple references case */
	multiref = &n1 , &n2;
	n1: node1 {
		ref = &{/node2}; /* reference precedes target */
		lref = &n2;
	};
	n2: node2 {
		ref = &{/node1}; /* reference after target */
		lref = &n1;
	};
	/* Check references to nested nodes with common prefix */
	foobar {
		n3: baz {
			ref = &{/foo/baz};
			lref = start: &n4 end:;
		};
	};
	foo {
		n4: baz {
			ref = &{/foobar/baz};
			lref = &n3;
		};
	};
	n5: bar {
		baz {
		};
	};
};

n6: &{n5/baz} {
	ref = &{n6/};
	lref = &{n5/baz};
};
