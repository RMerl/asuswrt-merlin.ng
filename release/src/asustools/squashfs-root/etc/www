//<![CDATA[
function to_submit(F)
{if(valid_value(F)){
F.submit_type.value="add_mac";
F.submit();
}
}
function to_apply(F)
{if(valid_value(F)){
F.submit_type.value="add_mac";
F.submit();
}
}
function valid_value(F)
{var num = F.elements.length;
var count = 0;
for(i=0;i<num;i++){
if(F.elements[i].type == "checkbox"){
if(F.elements[i].checked == true)
count = count + 1;
}
}
if(count > 128){
alert(errmsg.err44);
return false;
}
return true;
}
