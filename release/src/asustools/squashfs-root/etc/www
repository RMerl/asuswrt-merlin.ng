//<![CDATA[
function ttraff_restore_submit(F) {
if (F.file.value == "")	{
alert(errmsg.err42);
return false;
}
var len = F.file.value.length;
var ext = new Array('.','b','a','k');
var IMAGE = F.file.value.toLowerCase();
for (i=0; i < 4; i++)	{
if (ext[i] != IMAGE.charAt(len-4+i)){
alert(errmsg.err61);
return false;
}
}
apply(F);
}


//]]>
</script>
</head>
