{i}"hidden" name="submit_button" value="Traff_admin" />
<h2><% tran("status_inet.traff"); %>: <% tran("status_inet.dataadmin"); %></h2>
<br />		
{e}"setting">
{e}"label"><% tran("config.mess2"); %></div>
{i}"file" name="file" size="40" />
</div><br />
{e}"submitFooter">
{m}
//<![CDATA[
{x}{c}\"button\" type=\"button\" name=\"restore_button\" value=\"" + sbutton.restore + "\" onclick=\"ttraff_restore_submit(this.form);\" />");
{x}{c}\"button\" type=\"button\" name=\"close_button\" value=\"" + sbutton.clos + "\" onclick=\"self.close();\" />");
//]]>
</script>
</div>
</form>
</body>
