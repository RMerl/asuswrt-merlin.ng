clean-pciefd			:= $(patsubst %,%-clean,$(PCIEFD_TARGETS_LIST))
version-pciefd			:= $(patsubst %,%-version,$(PCIEFD_TARGETS_LIST))
ifeq ($(CONFIG_STBAP),y)
install-pciefd			:= $(patsubst %,%,$(PCIEFD_TARGETS_LIST))
endif # CONFIG_STBAP

