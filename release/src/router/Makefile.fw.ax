#endif

STBTARGET := ""
ifeq ($(CONFIG_STBAP),y)
STBTARGET := "-stb-hdmaaddr64"
endif # CONFIG_STBAP

# obj-pciefd expands to e.g. '43684b0-obj 4363c0-obj 43602a3-obj 4366c0-obj'
