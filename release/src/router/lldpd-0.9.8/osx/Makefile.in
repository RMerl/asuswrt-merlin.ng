install-launchddaemonsDATA: $(launchddaemons_DATA)
	@$(NORMAL_INSTALL)
	@list='$(launchddaemons_DATA)'; test -n "$(launchddaemonsdir)" || list=; \
	if test -n "$$list"; then \
	  echo " $(MKDIR_P) '$(DESTDIR)$(launchddaemonsdir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(launchddaemonsdir)" || exit 1; \
	fi; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(launchddaemonsdir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(launchddaemonsdir)" || exit $$?; \
	done

uninstall-launchddaemonsDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(launchddaemons_DATA)'; test -n "$(launchddaemonsdir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	dir='$(DESTDIR)$(launchddaemonsdir)'; $(am__uninstall_files_from_dir)
